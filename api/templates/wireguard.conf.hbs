# {{{ Self::CONFIG_NOTICE }}}

[Interface]
Address = {{{ self.wireguard.address }}}
PrivateKey = {{{ self.wireguard.private_key }}}
ListenPort = 51820

{{#each self.wireguard.peers }}
[Peer]
PublicKey = {{{ this.public_key }}}
AllowedIPs = {{{ this.allowed_ips }}}
{{#if this.route_internet }}AllowedIPs = {{{ crate::config::NetworkConfig::PUBLIC_SUBNETS }}}{{/if }}
Endpoint = {{{ this.endpoint }}}:{{{ this.endpoint_port }}}
PersistentKeepalive = 25
{{/each }}
